# cook your code here
avalaibleamt=120
bankcharge=0.50
balance=''
def test(n):
    if(n<=avalaibleamt and n%5==0):
        balance=avalaibleamt-n
        print(balance-bankcharge)
    elif(n>=avalaibleamt):
        print(avalaibleamt)
    else:
        print(avalaibleamt)


test(int(30))
test(int(42))
test(int(300))
